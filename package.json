{
  "name": "researchElectron",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FelicioAngga/ResearchElectron"
  },
  "build": {
    "appId": "ResearchElectron",
    "productName": "ResearchElectron",
    "directories": {
			"output": "electron-build",
			"buildResources": "build-assets"
		},
    "win": {
			"publish": [
				"github"
			],
      "extraResources": [
        "./*.dll"
      ]
		}
  },
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "build": "npx tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "electron-builder --win",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=x64 --asar --icon=assets/icons/SBS-64x64.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ResearchElectron\""
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/ffi-napi": "^4.0.6",
    "@types/node-os-utils": "^1.3.0",
    "@types/node-windows": "^0.1.2",
    "@types/wake_on_lan": "^0.0.30",
    "@types/winreg": "^1.2.31",
    "@types/wol": "^1.0.1",
    "electron": "^20.0.0",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.1.1",
    "electron-rebuild": "^3.2.9",
    "electron-winstaller": "^5.1.0",
    "tailwindcss": "^3.2.3",
    "typescript": "^4.8.4"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@journeyapps/sqlcipher": "^5.3.1",
    "@types/node": "^18.11.9",
    "active-win": "^7.7.2",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "ffi-napi": "^4.0.3",
    "node-active-window": "^0.1.0",
    "node-gyp": "^9.3.0",
    "node-windows": "^1.0.0-beta.8",
    "nodemon": "^2.0.20",
    "wake_on_lan": "^1.0.0",
    "winreg": "^1.2.4"
  }
}
